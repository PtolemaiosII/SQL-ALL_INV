	PRODUCT.NUM AS "sku",
	--'CA Website [CAD]' AS "tier_price_website",
	--'General' AS "tier_price_customer_group",
	2 AS "tier_price_website",
	0 AS "ALL GROUPS",
	1 AS "tier_price_customer_group",
