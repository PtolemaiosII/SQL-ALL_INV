	PRODUCT
	INNER JOIN PART ON PRODUCT.PARTID = PART.ID
	LEFT OUTER JOIN CustomVarChar AS "TB-Style" ON (PART.ID = "TB-Style".RECORDID AND "TB-Style".CUSTOMFIELDID = 42)
